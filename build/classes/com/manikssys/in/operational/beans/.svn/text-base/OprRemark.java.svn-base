package com.manikssys.in.operational.beans;

import java.io.Serializable;
import java.util.Date;

/**
 *
 * @author pc
 */
public class OprRemark implements Serializable {

    private int remarkId;
    private OprVesselPosition vesselPosition;
    private String srNo;
    private String remarkText;
    private Date remarkDate;

    public int getRemarkId() {
        return remarkId;
    }

    public void setRemarkId(int remarkId) {
        this.remarkId = remarkId;
    }

    public String getRemarkText() {
        return remarkText;
    }

    public void setRemarkText(String remarkText) {
        this.remarkText = remarkText;
    }

    public String getSrNo() {
        return srNo;
    }

    public void setSrNo(String srNo) {
        this.srNo = srNo;
    }

    public OprVesselPosition getVesselPosition() {
        return vesselPosition;
    }

    public void setVesselPosition(OprVesselPosition vesselPosition) {
        this.vesselPosition = vesselPosition;
    }

    public Date getRemarkDate() {
        return remarkDate;
    }

    public void setRemarkDate(Date remarkDate) {
        this.remarkDate = remarkDate;
    }


}
