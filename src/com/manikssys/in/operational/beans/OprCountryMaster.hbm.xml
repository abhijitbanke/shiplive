<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.manikssys.in.operational.beans.OprCountryMaster" schema="shipliveprod" table="opr_country_master">
    <id name="countryId" type="string">
      <column length="40" name="country_id"/>
    </id>
    <property name="countryCode" type="string">
      <column length="10" name="country_code" not-null="true"/>
    </property>
    <property name="countryName" type="string">
      <column length="25" name="country_name" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
